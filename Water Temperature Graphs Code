#August System
for(i in 1:nrow(DataLog20576731)){
  if(DataLog20576731[i,2]=="0:00"){DataLog20576731[i,2]=gsub("0:00","00:00",as.character(DataLog20576731[i,2]))}
  if(DataLog20576731[i,2]=="0:10"){DataLog20576731[i,2]=gsub("0:10","00:10",as.character(DataLog20576731[i,2]))}
