DataLog20576731$Time<-gsub("22:30:00","22:30",as.character(DataLog20576731$Time))
DataLog20576731$Time<-gsub("22:40:00","22:40",as.character(DataLog20576731$Time))
DataLog20576731$Time<-gsub("22:50:00","22:50",as.character(DataLog20576731$Time))
DataLog20576731$Time<-gsub("23:00:00","23:00",as.character(DataLog20576731$Time))
DataLog20576731$Time<-gsub("23:10:00","23:10",as.character(DataLog20576731$Time))
DataLog20576731$Time<-gsub("23:20:00","23:20",as.character(DataLog20576731$Time))
