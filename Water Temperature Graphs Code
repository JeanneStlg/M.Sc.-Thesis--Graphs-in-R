#these two days were removed from all graphs.

#Step 8.1: Remove the data from Day 4 and Day 18
Test1=Full.Data[(Full.Data$Date!="25/07/2022")==TRUE,]
Test2=Test1[(Test1$Date!="08/08/2022")==TRUE,]

#Step 8.2: Adapt the time to graph over 24 hours in ggplot

#Note: The date was chosen arbitrarily

Test2$Time=as.POSIXct(paste0("2022-07-30",Test2$Time,tz="GMT"))
Test3=Test2

#Step 8.3: Calculate the average temperature at the reference site at every hour

Test4=Test3[1:24,1:2]
names(Test4)=c("Average","Time")
rownames(Test4)=c(1:24)

Test4[,1]=c(mean(Test3[which(Test3$Time=="2022-07-30 00:00:00"&Test3$Site=="Reference"),3]),
            mean(Test3[which(Test3$Time=="2022-07-30 01:00:00"&Test3$Site=="Reference"),3]),
            mean(Test3[which(Test3$Time=="2022-07-30 02:00:00"&Test3$Site=="Reference"),3]),
            mean(Test3[which(Test3$Time=="2022-07-30 03:00:00"&Test3$Site=="Reference"),3]),
            mean(Test3[which(Test3$Time=="2022-07-30 04:00:00"&Test3$Site=="Reference"),3]),
