###
#Step 4: Process Data Logger 20576731
###

#Step 4.1: Select data specific to TidbiT 20576731
DataLog20576731=TidBit_Data[,1:13]
DataLog20576731=DataLog20576731[,-c(2:12)]

#Step 4.2: Adapt date and time
#Note: the data was saved under two different formats
##Data of July 2022 was saved as "DD/MM/YY HH:MM:SS" for every 10 minutes
##Data of August 2022 was saved as "D/M/YYYY H:MM" for every 10 minutes
#This step adapts the code in two columns: one for the date and one for the time
#One uniform format for all dates and times was selected
##Date: "DD/MM/YYYY"
##Time: "HH:MM"

#Double the first column
DataLog20576731=cbind(DataLog20576731$`TidBit Number`,DataLog20576731)

#Adapt column names
names(DataLog20576731)=c("Date","Time","Temperature")

#Remove the date from the time column
DataLog20576731$Time<-gsub("25/07/22 ","",as.character(DataLog20576731$Time))
DataLog20576731$Time<-gsub("26/07/22 ","",as.character(DataLog20576731$Time))
DataLog20576731$Time<-gsub("27/07/22 ","",as.character(DataLog20576731$Time))
DataLog20576731$Time<-gsub("28/07/22 ","",as.character(DataLog20576731$Time))
DataLog20576731$Time<-gsub("29/07/22 ","",as.character(DataLog20576731$Time))
DataLog20576731$Time<-gsub("30/07/22 ","",as.character(DataLog20576731$Time))
DataLog20576731$Time<-gsub("31/07/22 ","",as.character(DataLog20576731$Time))
DataLog20576731$Time<-gsub("1/8/2022 ","",as.character(DataLog20576731$Time))
DataLog20576731$Time<-gsub("2/8/2022 ","",as.character(DataLog20576731$Time))
DataLog20576731$Time<-gsub("3/8/2022 ","",as.character(DataLog20576731$Time))
DataLog20576731$Time<-gsub("4/8/2022 ","",as.character(DataLog20576731$Time))
DataLog20576731$Time<-gsub("5/8/2022 ","",as.character(DataLog20576731$Time))
DataLog20576731$Time<-gsub("6/8/2022 ","",as.character(DataLog20576731$Time))
DataLog20576731$Time<-gsub("7/8/2022 ","",as.character(DataLog20576731$Time))
DataLog20576731$Time<-gsub("8/8/2022 ","",as.character(DataLog20576731$Time))

